# Environment variables for Obsidian-Supernote Sync
# Copy this file to .env and fill in your values

# Supernote Cloud credentials (if using cloud sync)
SUPERNOTE_EMAIL=your-email@example.com
SUPERNOTE_PASSWORD=your-password-here

# Google Gemini API key (if using Gemini for OCR)
GOOGLE_API_KEY=your-google-api-key-here

# Azure Computer Vision (if using Azure for OCR)
AZURE_CV_KEY=your-azure-key-here
AZURE_CV_ENDPOINT=https://your-resource.cognitiveservices.azure.com/

# Paths
OBSIDIAN_VAULT_PATH=C:/Users/Edwin/Documents/ObsidianVault
SUPERNOTE_MOUNT_POINT=E:/
INTERMEDIATE_STORAGE=C:/Users/Edwin/Documents/ObsidianSupernoteSync
